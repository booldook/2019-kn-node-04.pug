<!DOCTYPE html>
html(lang="ko")
	include ../inc/header.pug
	body
		include ../inc/navi.pug
		div.container
			div.jumbotron.my-3
				h1= tit
					span= " - "
					small= subTit
			table.table.table-bordered.table-hover
				thead.thead-dark
					tr
						th 번호
						th 이름
						th 나이
						th 가입일
						th 관리
				tbody
					each item in datas
						tr
							td= item["id"]
							td= item["username"]
							td= item["age"]
							td= item["wdate"]
							td
								button.btn.btn-danger.btn-sm(onclick=`location.href='/user/rm/${item["id"]}';`) 삭제
								button.btn.btn-success.btn-sm(onclick=`location.href='/user/up/${item["id"]}';`) 수정


					
